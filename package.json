  "name": "openworld-express",
  "version": "1.0.0",
  "description": "Openworld Express.js API",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && ENV_FILE=./config/.env.prod node -r tsconfig-paths/register ./bin/app.js",
