{
  "name": "openworld-express",
  "version": "1.0.0",
  "description": "Openworld Express.js API",
  "main": "index.js",
  "scripts": {
    "**** BUILD ****": "",
    "bundle:swagger": "swagger-cli bundle config/openapi/_index.yaml --outfile config/openapi.yaml --type yaml",
    "build": "npm run bundle:swagger && tsc",
    "**** SERVER STARTUP ****": "",
    "start": "npm run build && cross-env ENV_FILE=./config/.env.prod node ./build/app.js",
    "start:dev": "npm run bundle:swagger && cross-env ENV_FILE=./config/.env.dev ts-node ./src/app.ts",
    "start:dev:reseed": "npm run seed:dev && npm run start:dev",
    "start:test": "npm run bundle:swagger && cross-env ENV_FILE=./config/.env.test ts-node ./src/app.ts",
    "**** GENERATORS ****": "",
    "typedoc": "typedoc src/api/dto/*.ts src/api/dto/**/*.ts --json build/typedoc.json",
    "generate:client:entities": "npm run typedoc && cross-env ENV_FILE=./config/.env.cli ts-node ./src/scripts/GenerateClientEntities.ts",
    "generate:client:communicator": "cross-env ENV_FILE=./config/.env.cli ts-node ./src/scripts/GenerateCommunicator.ts",
    "generate:server:models": "cross-env ENV_FILE=./config/.env.cli ts-node ./src/scripts/GenerateServerModels.ts",
    "generate:server:controllers": "cross-env ENV_FILE=./config/.env.cli ts-node ./src/scripts/GenerateServerControllers.ts",
    "generate:server": "npm run generate:server:models && npm run generate:server:controllers",
    "generate:client": "npm run generate:client:entities && npm run generate:client:communicator",
    "**** TESTS ****": "",
    "test:all": "npm run seed:test && npm run test:model && npm run test:api",
    "test:model": "cross-env ENV_FILE=./config/.env.test jest --coverage src/tests/models",
    "test:api": "start-server-and-test start:test http://localhost:3000 test:controller",
    "test:controller": "cross-env ENV_FILE=./config/.env.test jest --coverage src/tests/controllers",
    "test:unit": "npm run seed:test && cross-env ENV_FILE=./config/.env.test jest --coverage",
    "test:verbose": "npm run seed:test && cross-env ENV_FILE=./config/.env.test jest --verbose --coverage",
    "**** DATA ****": "",
    "seed:test": "npm run schema:drop:test && cross-env ENV_FILE=./config/.env.test ts-node ./src/utils/seedTestData.ts",
    "seed:dev": "npm run schema:drop:dev && cross-env ENV_FILE=./config/.env.dev ts-node ./src/utils/seedDevelopmentData.ts",
    "schema:drop:test": "ts-node ./node_modules/typeorm/cli.js schema:drop -c test",
    "schema:sync:test": "ts-node ./node_modules/typeorm/cli.js schema:sync -c test",
    "schema:drop:dev": "ts-node ./node_modules/typeorm/cli.js schema:drop -c dev",
    "schema:sync:dev": "ts-node ./node_modules/typeorm/cli.js schema:sync -c dev",
    "**** CODE FORMATTING ****": "",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/dotenv-parse-variables": "^0.2.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/socket.io": "^2.1.13",
    "@types/validator": "^13.7.0",
    "@types/yamljs": "^0.2.31",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "connect": "^3.7.0",
    "dotenv-extended": "^2.9.0",
    "dotenv-parse-variables": "^1.0.1",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.13.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.6",
    "mysql": "^2.14.1",
    "reflect-metadata": "^0.1.10",
    "socket.io": "^3.1.2",
    "swagger-cli": "^4.0.4",
    "swagger-routes-express": "^3.3.1",
    "tsconfig-paths": "^3.11.0",
    "typeorm": "0.2.29",
    "validator": "^13.7.0",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/jest": "^26.0.24",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.17.33",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "axios": "^0.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "jest-openapi": "^0.14.2",
    "prettier": "^2.5.1",
    "sqlite3": "^4.2.0",
    "start-server-and-test": "^1.14.0",
    "supertest": "^6.1.6",
    "swagger-parser": "^10.0.3",
    "ts-jest": "^26.5.6",
    "ts-node": "3.3.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.4.4"
  }
}
